void hasil(){
    String Nama,Npm,Alamat,NoHp,Email;
    Nama=jTextField2Nama.getText();
    Npm=jTextField1Npm.getText();
    Alamat=jTextArea1Alamat.getText();
    NoHp=jTextField3NoHp.getText();
    Email=jTextField4Email.getText();
    
    String hasil = ""+Nama+
                   "\n"+Npm+
                   "\n"+Alamat+
                   "\n"+NoHp+
                   "\n"+Email;
    jTextArea2.setText(hasil);
}
}
